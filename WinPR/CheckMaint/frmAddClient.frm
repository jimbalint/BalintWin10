Private Sub cmdSaveClient_Click()
    
    Client.OpenRS
    Client.Clear
    Client.ClientName = Me.TDBName
    Client.Prefix = Me.TDBPrefix
    Client.Contact = Me.TDBContact
    Client.Phone = Me.TDBPhone
    Client.Message1 = Me.TDBMsg1
    Client.Message2 = Me.TDBMsg2
    Client.Message3 = Me.TDBMsg3
    Client.Save (RecAdd)
    Me.TDBName.Text = Client.ClientName
    Unload Me

End Sub

