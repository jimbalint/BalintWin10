    Customer.Address1 = Me.TDBAddr1
    Customer.Address2 = Me.TDBAddr2
    Customer.Address3 = Me.TDBAddr3
    Customer.Address4 = Me.TDBAddr4
    Customer.Bank1 = Me.TDBBank1
    Customer.Bank2 = Me.TDBBank2
