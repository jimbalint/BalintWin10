Option Explicit

Dim rs As New ADODB.Recordset

Dim I, J, K As Long


Private Sub Form_Load()
    
    rs.CursorLocation = adUseClient
    rs.CursorType = adOpenKeyset
    rs.Fields.Append "Select", adBoolean
    rs.Fields.Append "Number", adDouble
    rs.Fields.Append "Name", adVarChar, 30, adFldIsNullable
    rs.Open , , adOpenDynamic, adLockOptimistic
    
    For I = 1 To 10
        rs.AddNew
        rs!Select = True
        rs!Number = I * 100 + I
        rs!Name = "ABC " & I
        rs.Update
    Next I
    
    fg.FixedCols = 0                   ' see all cols selected by SQL
    fg.FocusRect = flexFocusSolid      ' Cell apearance when editable & in focus
    
    fg.DataMode = flexDMBound          ' Recordset cursor is maintained by grid
    ' Set fg.DataSource = rs.DataSource '
    Set fg.DataSource = rs.DataSource
    ' fg.DataMember = rs.DataMember     '

    fg.Editable = flexEDKbdMouse       ' Edit by keys or mouse picks
    fg.BackColorAlternate = RGB(192, 192, 192)          ' light gray
    fg.TabBehavior = flexTabCells                       ' tab moves between cells
    fg.AllowSelection = False                          ' don't allow selection of ranges of cells

    Me.KeyPreview = True

End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
    Select Case KeyCode
        Case vbKeyEscape: cmdExit_Click
    End Select
End Sub

Private Sub cmdExit_Click()
    End
End Sub

