Option Explicit

Dim i, j, k As Long

Dim rs As New ADODB.Recordset
Dim rsDrop As New ADODB.Recordset

Dim CCDrop As String

Private Sub fg_AfterRowColChange(ByVal OldRow As Long, ByVal OldCol As Long, ByVal NewRow As Long, ByVal NewCol As Long)
    
Dim TxtMtrx As String

    If fg.Rows = 1 Then Exit Sub
    If OldCol <> 2 Then Exit Sub
    
    TxtMtrx = Trim(fg.TextMatrix(OldRow, OldCol) & "")
    If TxtMtrx = "" Then Exit Sub
    
    rsDrop.Find "Description = '" & TxtMtrx & "'", 0, adSearchForward, 1
    If rsDrop.EOF Then
        
        k = k + 1
        rsDrop.AddNew
        rsDrop!Description = Trim(TxtMtrx)
        rsDrop!Number = k
        rsDrop.Update
        
        If rsDrop.RecordCount > 1 Then
            rsDrop.Sort = "Description"
        End If
        
        CCDrop = ""
        
        rsDrop.MoveFirst
        Do
            CCDrop = CCDrop & "|" & rsDrop!Description
            rsDrop.MoveNext
        Loop Until rsDrop.EOF
    
