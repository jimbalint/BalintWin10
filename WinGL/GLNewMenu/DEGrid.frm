Dim xdb As New XArrayDB

Private Sub cmdAdd_Click()
    
    If TDBGrid1.Bookmark = 1 Then
        xdb.InsertRows TDBGrid1.Bookmark
        xdb.Value(TDBGrid1.Bookmark, 5) = "ADD"
    Else
        xdb.InsertRows TDBGrid1.Bookmark + 1
        xdb.Value(TDBGrid1.Bookmark + 1, 5) = "ADD"
        TDBGrid1.Bookmark = TDBGrid1.Bookmark + 1
    End If
    TDBGrid1.ReBind
    TDBGrid1.SetFocus

End Sub

Private Sub CmdExit_Click()
    Unload Me
End Sub

Private Sub cmdSave_Click()
    Dim xFactory As New cFactory
    Me.TDBGrid1.Update
    Me.TDBGrid1.MousePointer = dbgMPHourglass
    xFactory.PutHist MainMenu.lblFileName, frmDEDate.FiscalYear, frmDEDate.Period, xdb
    Me.TDBGrid1.MousePointer = vbDefault
    Me.TDBGrid1.SetFocus
End Sub

Private Sub Form_Load()
    
    Set xdb = xFactory.GetHist(MainMenu.lblFileName, frmDEDate.FiscalYear, frmDEDate.Period)
    
    Set Me.TDBGrid1.Array = xdb
    
    Me.TDBGrid1.Columns(2).Width = 3000
    Me.TDBGrid1.Columns(3).Width = 3000
    
End Sub
